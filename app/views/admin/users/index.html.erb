<h1>users</h1>

<%= link_to "Create user &raquo;", new_admin_user_path %>

<table>
  <tr>
    <th>Username</th>
    <th></th>
  </tr>
<% @users.each do |user| %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td><%= user.username %></td>
    <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
  </tr>
<% end %>
</table>